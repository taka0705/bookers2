<%= render 'books/list',user: @user %>

<h2>Users</h2>
<table>
<tr>
    <th>image</th>
    <th>name</th>
    </tr>
<% @users.each do |book| %>
<tr>
  <td><%= image_tag book.get_profile_image(100,100) %></td>
  <td><%= book.name  %></td>
  <td><%= link_to "Show",user_path(book.id) %></td>
  </tr>
<% end %>
</table>