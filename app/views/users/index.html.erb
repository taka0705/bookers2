<div class="container px-5 px-sm-0">
  <div class="row">
    <div class="col-md-3">

<%= render 'books/list',user: @user %>
    </div>

<div class="col-md-8 offset-md-1">
<h2>Users</h2>
  <table class="table">
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
      </tr>
    </thead>
<% @users.each do |book| %>
<tr>
  <td><%= image_tag book.get_profile_image(100,100) %></td>
  <td><%= book.name  %></td>
  <td><%= link_to "Show",user_path(book.id) %></td>
  </tr>
<% end %>
  </table>
</div>
  </div>
</div>